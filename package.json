{
  "name": "my_bn",
  "description": "Barnes & Noble App",
  "version": "1.2.5",
  "private": true,
  "scripts": {
    "lint-fix": "eslint --fix ./src ./native_modules --ext .ts,.tsx,.js,.jsx",
    "lint": "eslint ./src ./native_modules --ext .ts,.tsx,.js,.jsx",
    "lint-watch": "esw -w --changed ./src ./native_modules --ext .ts,.tsx,.js,.jsx",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "postinstall": "./scripts/post.npm.install.sh",
    "start": "react-native start",
    "reset-cache": "react-native start --reset-cache",
    "set-dev": "node ./config/set-config.js dev",
    "set-prod": "node ./config/set-config.js prod",
    "test": "jest",
    "release-android": "./scripts/release.android.sh",
    "version:patch": "npm --no-git-tag-version version patch",
    "git:add": "git add package.json && git add package-lock.json",
    "increase-version": "npm run version:patch &&  npm run git:add && git commit -m 'Incremented version'"
  },
